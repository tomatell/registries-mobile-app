<?xml version='1.0' encoding='utf-8'?>
<widget id="com.unionsoft.MemberyMobile" version="0.0.1"
	xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>Membery Mobile</name>
	<description>
		Membery Mobile Client.
	</description>
	<author email="info@unionsoft.sk" href="http://www.unionsoft.sk">
		UnionSoft s.r.o.
	</author>
	<content src="https://caihp.unionsoft.sk/" />
	<plugin name="cordova-plugin-whitelist" version="1" />
		<access origin="*" />
		<allow-intent href="http://*/*" />
		<allow-intent href="https://*/*" />
		<allow-intent href="tel:*" />
		<allow-intent href="sms:*" />
		<allow-intent href="mailto:*" />
		<allow-intent href="geo:*" />
		<allow-navigation href="http://*/*" />
		<allow-navigation href="https://*/*" />
		<allow-navigation href="tel:*" />
		<allow-navigation href="sms:*" />
		<allow-navigation href="mailto:*" />
		<allow-navigation href="geo:*" />
		<platform name="android">
			<allow-intent href="market:*" />
		</platform>
		<platform name="ios">
			<allow-intent href="itms:*" />
			<allow-intent href="itms-apps:*" />
		</platform>
</widget>

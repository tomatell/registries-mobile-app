        <icon height="58" src="res/wp8/icons/ApplicationIcon-58x58.png" width="58" />
        <icon height="62" src="res/wp8/icons/ApplicationIcon-62x62.png" width="62" />
        <icon height="70" src="res/wp8/icons/ApplicationIcon-70x70.png" width="70" />
        <icon height="71" src="res/wp8/icons/ApplicationIcon-71x71.png" width="71" />
        <icon height="99" src="res/wp8/icons/ApplicationIcon-99x99.png" width="99" />

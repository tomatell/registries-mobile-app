        <splash height="1242" src="res/ios/splash/Default-Landscape-736h.png" width="2208" />
    </platform>
    <platform name="wp8">
        <preference name="windows-target-version" value="8.0" />
        <hook type="after_platform_add" src="hooks/windows_setup_after_adding_platform.js" />
        <hook type="before_platform_add" src="hooks/windows_setup_before_adding_platform.js" />
        <icon src="res/windows/icons/ApplicationIcon-24x24.png" />
        <icon height="24" src="res/wp8/icons/ApplicationIcon-24x24.png" width="24" />
        <icon height="33" src="res/wp8/icons/ApplicationIcon-33x33.png" width="33" />
